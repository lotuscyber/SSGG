    time.sleep(0.05)
    print "\033[1;97m[1>>>]\x1b[1;94mCLONING WITHOUT LOGIN( \033[1;92m NOW)"
    time.sleep(0.05)
    print '\x1b[1;94m[0>>>]\033[1;91m Exit ( Back)'
    print'\033[1;92mâ—â”â”â”â”â”â”â”â”â—â—â”â”â”â”â”â”â”â”â—â—â”â”â”â”â”â”â”â”â—'
    pilih_login()

def pilih_login():
    peak = raw_input("\n\033[1;94mCHOOSE: \033[1;93m")
    if peak =="":
        print "\x1b[1;97mFill In Correctly"
        pilih_login()
    elif peak =="1":
        Zeek()
def Zeek():
    os.system('clear')
    print logo2
    print'â—â”â”â”â”â”â”â”â”â—â—â”â”â”â”â”â”â”â”â—'
    print '\x1b[1;96m[1>>>]  Pakistan'
    time.sleep(0.10)
    print '\x1b[1;94m[0] back'
    print'â—â”â”â”â”â”â”â”â”â—â—â”â”â”â”â”â”â”â”â—'

    time.sleep(0.05)
    action()

def action():
    peak = raw_input('\n\033[1;93mCHOOSE:\033[1;93m')
    if peak =='':
        print '[!] Fill In Correctly'
        action()
    elif peak =="1":
        os.system("clear")
        print logo2

