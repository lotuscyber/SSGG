def login():
    os.system('clear')
    print logo1
    print "\033[1;91m*â—â—â—â—â—â—â—â—â—â—â—â—â—â—â—â—â—â—â—â—â—â—â—â—â—â—â—â—â—â—*"
    time.sleep(0.05)
