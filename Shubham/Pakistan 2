CorrectUsername = "pak"
CorrectPassword = "pak"

loop = 'true'
while (loop == 'true'):
    username = raw_input("\033[1;97m\x1b[1;96mBRAND NAME\x1b[1;93mâ–¶\x1b[1;97m")
    if (username == CorrectUsername):
        password = raw_input("\033[1;97m \x1b[1;95mBRAND PASSWORD \x1b[1;93mâ–¶\x1b[1;97m")
        if (password == CorrectPassword):
            print "Logged in successfully as " + username #Dev:MAFIA-KILLER
            time.sleep(2)
            loop = 'false'
        else:
            print "\033[1;94mWrong Password"
            os.system('xdg-open https://m.facebook.com')
    else:
        print "\033[1;94mWrong Username"
        os.system('xdg-open https://m.facebook.com')

